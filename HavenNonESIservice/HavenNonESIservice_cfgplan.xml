<SOAConfigPlan xmlns:jca="http://platform.integration.oracle/blocks/adapter/fw/metadata" xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" xmlns:orawsp="http://schemas.oracle.com/ws/2006/01/policy" xmlns:edl="http://schemas.oracle.com/events/edl" xmlns="http://schemas.oracle.com/soa/configplan">
   <composite name="HavenNonESIservice">
      <!--Add search and replace rules for the import section of a composite
	Example:
	<searchReplace>
		<search>http://my-dev-server</search>
		<replace>http://my-test-server</replace>
	<searchReplace>
	<searchReplace>
		<search>8888</search>
		<replace>8889</replace>
	<searchReplace>-->
      <import>
         <searchReplace>
            <search/>
            <replace/>
         </searchReplace>
      </import>
      <service name="ReceiveRequestFromHaven">
         <binding type="jca"/>
      </service>
      <property name="productVersion">
         <replace>11.1.1.7.8</replace>
      </property>
      <!--Add search and replace rules for the component properties
	For components and service/reference bindings, you can add policy references.
	Example:
	<component name="*">
		<wsp:PolicyReference orawsp:category="management" orawsp:status="enabled" URI="oracle/log_policy"/>
	</component>-->
      <component name="HavenNonESIServiceBpel">
         <property name="bpel.config.transaction">
            <replace>required</replace>
         </property>
         <property name="bpel.config.oneWayDeliveryPolicy">
            <replace>sync</replace>
         </property>
      </component>
      <reference name="InvokeFedHub">
         <binding type="ws">
            <attribute name="port">
               <replace>http://vnem.ee.sim.dsh.cms.hhs.gov#wsdl.endpoint(VerifyNonEsiMecService/VerifyNonEsiMecPort)</replace>
            </attribute>
            <attribute name="location">
               <searchReplace>
                  <search>VerifyNonESIFedProxyService.wsdl</search>
                  <replace>http://10.82.55.26:8011/FedHub/VerifyNonESI/FedProxyService?wsdl</replace>
               </searchReplace>
            </attribute>
            <property name="oracle.soa.ws.outbound.omitWSA">
               <replace>true</replace>
            </property>
            <property name="weblogic.wsee.wsat.transaction.flowOption">
               <replace>WSDLDriven</replace>
            </property>
            <property name="oracle.webservices.httpReadTimeout">
               <replace>30000</replace>
            </property>
            <property name="csf-key">
               <searchReplace>
                  <search>osb.system.user</search>
                  <replace>osb.system.user</replace>
               </searchReplace>
            </property>
         </binding>
      </reference>
      <reference name="ProduceResponseToHaven">
         <binding type="jca"/>
      </reference>
   </composite>
