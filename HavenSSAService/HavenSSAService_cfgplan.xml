   <composite name="HavenSSAService">
      <!--Add search and replace rules for the import section of a composite
	Example:
	<searchReplace>
		<search>http://my-dev-server</search>
		<replace>http://my-test-server</replace>
	<searchReplace>
	<searchReplace>
		<search>8888</search>
		<replace>8889</replace>
	<searchReplace>-->
      <import>
         <searchReplace>
            <search/>
            <replace/>
         </searchReplace>
      </import>
      <service name="ConsumeMessageFromHaven">
         <binding type="jca"/>
      </service>
      <property name="productVersion">
         <replace>11.1.1.7.8</replace>
      </property>
      <!--Add search and replace rules for the component properties
	For components and service/reference bindings, you can add policy references.
	Example:
	<component name="*">
		<wsp:PolicyReference orawsp:category="management" orawsp:status="enabled" URI="oracle/log_policy"/>
	</component>-->
      <component name="HavenSSAVBpel">
         <property name="bpel.config.transaction">
            <replace>required</replace>
         </property>
         <property name="bpel.config.oneWayDeliveryPolicy">
            <replace>sync</replace>
         </property>
      </component>
      <reference name="ProduceResponseTohaven">
         <binding type="jca"/>
      </reference>
      <reference name="InvokeFedHub">
         <!--Add search and replace rules for the binding properties-->
         <binding type="ws">
            <attribute name="port">
               <replace>http://extn.ssac.ee.sim.dsh.cms.hhs.gov#wsdl.endpoint(SSACompositeService/SSACompositePort)</replace>
            </attribute>
            <attribute name="location">
              <searchReplace>
                  <search>SSAFedProxyService.wsdl</search>
                  <replace>http://dhsdcaswlesb001.dhs.dc.gov:8011/FedHub/SSA/FedProxyService?wsdl</replace>
               </searchReplace>
            </attribute>
            <property name="weblogic.wsee.wsat.transaction.flowOption">
               <replace>WSDLDriven</replace>
            </property>
            <property name="oracle.webservices.httpReadTimeout">
               <replace>30000</replace>
            </property>
            <property name="oracle.soa.ws.outbound.omitWSA">
               <replace>true</replace>
            </property>
            <property name="csf-key">
               <replace>osb.system.user</replace>
            </property>
         </binding>
      </reference>
   </composite>
