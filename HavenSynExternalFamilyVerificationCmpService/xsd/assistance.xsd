                                    </xs:sequence>
                                 </xs:complexType>
                              </xs:element>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                     <xs:group ref="ResourceTimestampGroup"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="participant_id" type="xs:string"/>
   <xs:element name="person_id" type="xs:string"/>
   <xs:complexType name="CuramApplicantType">
      <xs:sequence>
         <xs:element name="identifier_sets">
