         <xs:element name="gender" type="xs:string"/>
         <xs:element name="ssn" type="xs:string"/>
         <xs:element name="aptc_amount" minOccurs="0" type="xs:string"/>
         <xs:element name="csr" minOccurs="0" type="xs:string"/>
