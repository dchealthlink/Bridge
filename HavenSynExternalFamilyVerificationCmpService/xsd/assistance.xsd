      <xs:annotation>
         <xs:documentation>Comment describing your root element</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="integrated_case" maxOccurs="unbounded">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="integrated_case_reference" type="xs:string"/>
