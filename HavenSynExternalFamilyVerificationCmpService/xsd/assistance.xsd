      <xs:complexType>
         <xs:sequence>
            <xs:element ref="curam_applicant" maxOccurs="unbounded"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="hbx_person_id" type="xs:anyURI"/>
