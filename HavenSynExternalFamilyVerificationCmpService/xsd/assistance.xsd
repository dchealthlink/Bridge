                                    </xs:sequence>
                                 </xs:complexType>
                              </xs:element>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="product_delivery_cases">
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="product_delivery_case" maxOccurs="unbounded">
