<?xml version="1.0" encoding="UTF-8"?>
<SOAConfigPlan xmlns:jca="http://platform.integration.oracle/blocks/adapter/fw/metadata"
               xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
               xmlns:orawsp="http://schemas.oracle.com/ws/2006/01/policy"
               xmlns:edl="http://schemas.oracle.com/events/edl"
               xmlns="http://schemas.oracle.com/soa/configplan">
   <composite name="HavenESIMEConsumeService">
      <!--Add search and replace rules for the import section of a composite
	Example:
	<searchReplace>
		<search>http://my-dev-server</search>
		<replace>http://my-test-server</replace>
	<searchReplace>
	<searchReplace>
		<search>8888</search>
		<replace>8889</replace>
	<searchReplace>-->
      <import>
         <searchReplace>
            <search/>
            <replace/>
         </searchReplace>
      </import>
      <service name="ConsumeESIMECRequest">
         <binding type="jca"/>
      </service>
      <property name="productVersion">
         <replace>11.1.1.7.0</replace>
      </property>
      <!--Add search and replace rules for the component properties
	For components and service/reference bindings, you can add policy references.
	Example:
	<component name="*">
		<wsp:PolicyReference orawsp:category="management" orawsp:status="enabled" URI="oracle/log_policy"/>
	</component>-->
      <component name="HavenESIMECBpelService">
         <property name="bpel.config.transaction">
            <replace>required</replace>
         </property>
         <property name="bpel.config.oneWayDeliveryPolicy">
            <replace>sync</replace>
         </property>
      </component>
      <reference name="Invoke_ESIMECService">
         <!--Add search and replace rules for the binding properties-->
         <binding type="ws">
            <attribute name="port">
               <replace>http://gov.hhs.cms.hix.dsh.ee.esi_mec.ext#wsdl.endpoint(CalculateOPMPremiumService/CalculateOPMPremiumPortType)</replace>
            </attribute>
            <attribute name="location">
               <searchReplace>
                  <search>VerifyEsiMecFedProxyService.wsdl</search>
                  <replace>https://esb-preprd.dchealthlink.com/FedHub/VerifyEsiMec/FedProxyService?wsdl</replace>
               </searchReplace>
            </attribute>
            <property name="oracle.soa.ws.outbound.omitWSA">
               <replace>true</replace>
            </property>
            <property name="weblogic.wsee.wsat.transaction.flowOption">
               <replace>WSDLDriven</replace>
            </property>
            <property name="oracle.webservices.httpReadTimeout">
               <replace>30000</replace>
            </property>
            <property name="csf-key">
               <replace>osb.system.user</replace>
            </property>
         </binding>
      </reference>
      <reference name="PublishESIMECResponseJMS">
         <binding type="jca"/>
      </reference>
   </composite>
   <!--To configure monitor.config: 
	<property name="enabled"><replace>true</replace></property>
	<property name="dataObjectsFolder"><searchReplace><search>mydev</search><replace>myproduction</replace></searchReplace></property>
	
	sample properties to configure for JMS adapter: 
	<jca:property name="DestinationName"><replace>jms/DemoSupplierTopic</replace></jca:property>
	<jca:property name="TimeToLive"><replace>0</replace></jca:property>
	
	To add search and replace rules for wsdls, xsd and jca files
	Example:
	<searchReplace>
		<search>http://my-dev-server</search>
		<replace>http://my-test-server</replace>
	<searchReplace>
	<searchReplace>
		<search>8888</search>
		<replace>8889</replace>
	<searchReplace>
	-->
   <wsdlAndSchema name="ConsumeESIMECRequest.wsdl|ConsumeESIMECRequest_jms.jca|ESIMECFedProxyService.wsdl|ESIMECFedProxyServiceWrapper.wsdl|PublishESIMECResponseJMS.wsdl|PublishESIMECResponseJMS_jms.jca|RuntimeFault.wsdl|xsd/appinfo.xsd|xsd/appinfo_1.xsd|xsd/ExchangeModel.xsd|xsd/ExtensionModel.xsd|xsd/fbi.xsd|xsd/hix_core.xsd|xsd/hix_ee.xsd|xsd/niem_core.xsd|xsd/structures.xsd|xsd/usps_states.xsd|xsd/xsd.xsd">
      <searchReplace>
         <search/>
         <replace/>
      </searchReplace>
   </wsdlAndSchema>
</SOAConfigPlan>
