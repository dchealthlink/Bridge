<process name="HavenESIMECBpelService"
               targetNamespace="http://xmlns.oracle.com/SOA_APP/HavenESIMEConsumeService/HavenESIMECBpelService"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/SOA_APP/HavenESIMEConsumeService/HavenESIMECBpelService"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/jms/SOA_APP/HavenESIMEConsumeService/ConsumeESIMECRequest"
         xmlns:ns2="http://gov.hhs.cms.hix.dsh.ee.esi_mec.ext"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/jms/SOA_APP/HavenESIMEConsumeService/PublishESIMECResponseJMS"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns6="http://vesim.dsh.cms.gov/extension/1.0"
         xmlns:ns7="http://niem.gov/niem/niem-core/2.0"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns5="http://niem.gov/niem/structures/2.0"
         xmlns:ns4="http://vesim.dsh.cms.gov/exchange/1.0"
         xmlns:ns8="http://hix.cms.gov/0.1/hix-ee"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns9="http://xmlns.oracle.com/pcbpel/adapter/jms/SOA_APP/HavenESIMEConsumeService/PublishErrorResp"
         xmlns:ns10="http://hix.cms.gov/0.1/hix-core">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://schemas.oracle.com/bpel/extension"
          location="RuntimeFault.wsdl"
          importType="http://schemas.xmlsoap.org/wsdl/"/>
  <partnerLinks>
    <partnerLink name="ConsumeESIMECRequest"
                 partnerLinkType="ns1:Consume_Message_plt"
                 myRole="Consume_Message_role"/>
    <partnerLink name="Invoke_ESIMECService"
                 partnerLinkType="ns2:Invoke_ESIMECService"
                 partnerRole="CalculateOPMPremiumPortType"/>
    <partnerLink name="PublishESIMECResponseJMS"
                 partnerLinkType="ns3:Produce_Message_plt"
                 partnerRole="Produce_Message_role"/>
  </partnerLinks>
  <variables>
    <variable name="Consume_Message_InputVariable"
              messageType="ns1:Consume_Message_msg"/>
    <variable name="Invoke_FEDESIService_InputVariable"
              messageType="ns2:CalculateOPMPremiumRequest"/>
    <variable name="Invoke_FEDESIService_OutputVariable"
              messageType="ns2:CalculateOPMPremiumResponse"/>
    <variable name="Invoke_PublishESIResptoJMS_InputVariable"
              messageType="ns3:Produce_Message_msg"/>
    <variable name="Title" type="xsd:string"/>
    <variable name="FaultVar" messageType="bpelx:RuntimeFaultMessage"/>
  </variables>
  <faultHandlers>
    <catch faultName="bpelx:remoteFault" faultVariable="FaultVar"
           faultMessageType="bpelx:RuntimeFaultMessage">
      <sequence name="Sequence3">
        <assign name="AssignRemote">
          <copy>
            <from>$FaultVar.code</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns6:ResponseMetadata/ns10:ResponseCode</to>
          </copy>
          <copy>
            <from>$FaultVar.summary</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns6:ResponseMetadata/ns10:ResponseDescriptionText</to>
          </copy>
          <copy>
            <from>$Consume_Message_InputVariable.body/ns6:ApplicantRequest/ns6:RequestNumber</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns4:RequestNumber</to>
          </copy>
        </assign>
        <assign name="TransformRemote">
          <bpelx:annotation>
            <bpelx:pattern patternName="bpelx:transformation"/>
          </bpelx:annotation>
          <copy>
            <from>ora:doXSLTransformForDoc("xsl/XformRemoteFault.xsl", $Invoke_PublishESIResptoJMS_InputVariable.body)</from>
            <to variable="Invoke_PublishESIResptoJMS_InputVariable"
                part="body"/>
          </copy>
        </assign>
        <invoke name="InvokePublishErrorResp"
                partnerLink="PublishESIMECResponseJMS"
                portType="ns3:Produce_Message_ptt" operation="Produce_Message"
                inputVariable="Invoke_PublishESIResptoJMS_InputVariable"
                bpelx:invokeAsDetail="no"/>
      </sequence>
    </catch>
    <catch faultName="bpelx:bindingFault" faultVariable="FaultVar"
           faultMessageType="bpelx:RuntimeFaultMessage">
      <sequence name="Sequence2">
        <assign name="AssignBinding">
          <copy>
            <from>$FaultVar.code</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns6:ResponseMetadata/ns10:ResponseCode</to>
          </copy>
          <copy>
            <from>$FaultVar.summary</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns6:ResponseMetadata/ns10:ResponseDescriptionText</to>
          </copy>
          <copy>
            <from>$Consume_Message_InputVariable.body/ns6:ApplicantRequest/ns6:RequestNumber</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns4:RequestNumber</to>
          </copy>
        </assign>
        <assign name="TransformBinding">
          <bpelx:annotation>
            <bpelx:pattern patternName="bpelx:transformation"/>
          </bpelx:annotation>
          <copy>
            <from>ora:doXSLTransformForDoc("xsl/XformBindingFault.xsl", $Invoke_PublishESIResptoJMS_InputVariable.body)</from>
            <to variable="Invoke_PublishESIResptoJMS_InputVariable"
                part="body"/>
          </copy>
        </assign>
        <invoke name="InvokePublishErrorResp"
                partnerLink="PublishESIMECResponseJMS"
                portType="ns3:Produce_Message_ptt" operation="Produce_Message"
                inputVariable="Invoke_PublishESIResptoJMS_InputVariable"
                bpelx:invokeAsDetail="no"/>
      </sequence>
    </catch>
    <catchAll>
      <sequence name="Sequence1">
        <assign name="AssignFault">
          <copy>
            <from>"ESIMECFaultCode"</from>
            <to>$FaultVar.code</to>
          </copy>
          <copy>
            <from>ora:getFaultAsString()</from>
            <to>$FaultVar.summary</to>
          </copy>
          <copy>
            <from>ora:getFaultName()</from>
            <to>$FaultVar.detail</to>
          </copy>
        </assign>
        <assign name="AssignGeneric">
          <copy>
            <from>$FaultVar.code</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns6:ResponseMetadata/ns10:ResponseCode</to>
          </copy>
          <copy>
            <from>$FaultVar.summary</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns6:ResponseMetadata/ns10:ResponseDescriptionText</to>
          </copy>
          <copy>
            <from>$Consume_Message_InputVariable.body/ns6:ApplicantRequest/ns6:RequestNumber</from>
            <to>$Invoke_PublishESIResptoJMS_InputVariable.body/ns4:RequestNumber</to>
          </copy>
        </assign>
        <assign name="TransformGeneric">
          <bpelx:annotation>
            <bpelx:pattern patternName="bpelx:transformation"/>
          </bpelx:annotation>
          <copy>
            <from>ora:doXSLTransformForDoc("xsl/XformRemoteError.xsl", $Invoke_PublishESIResptoJMS_InputVariable.body)</from>
            <to variable="Invoke_PublishESIResptoJMS_InputVariable"
                part="body"/>
          </copy>
        </assign>
        <invoke name="InvokePublishErrorResp"
                partnerLink="PublishESIMECResponseJMS"
                portType="ns3:Produce_Message_ptt" operation="Produce_Message"
                inputVariable="Invoke_PublishESIResptoJMS_InputVariable"
                bpelx:invokeAsDetail="no"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <sequence name="main">
    <receive name="ReceiveInput" createInstance="yes"
             partnerLink="ConsumeESIMECRequest"
             portType="ns1:Consume_Message_ptt" operation="Consume_Message"
             variable="Consume_Message_InputVariable"/>
    <assign name="AssignTitle">
      <copy>
        <from>concat($Consume_Message_InputVariable.body/ns6:ApplicantRequest/ns6:RequestPerson/ns7:PersonName/ns7:PersonGivenName,' ',$Consume_Message_InputVariable.body/ns6:ApplicantRequest/ns6:RequestPerson/ns7:PersonName/ns7:PersonSurName)</from>
        <to>$Title</to>
      </copy>
    </assign>
    <extensionActivity>
      <bpelx:exec name="Java_Embedding" language="java">
        <![CDATA[/*Write your java code below e.g. 
	System.out.println("Hello, World");
*/
String title = "Person Name:"+(String)getVariableData("Title");        
setCompositeInstanceTitle(title);]]>
      </bpelx:exec>
    </extensionActivity>
    <assign name="Transform_Req">
      <bpelx:annotation>
        <bpelx:pattern patternName="bpelx:transformation"/>
      </bpelx:annotation>
      <copy>
        <from>ora:doXSLTransformForDoc("xsl/XformReqtoFEDESI.xsl", $Consume_Message_InputVariable.body)</from>
        <to variable="Invoke_FEDESIService_InputVariable" part="body"/>
      </copy>
    </assign>
    <invoke name="Invoke_FEDESIService"
            partnerLink="Invoke_ESIMECService"
            portType="ns2:CalculateOPMPremiumPortType"
            operation="CalculateOPMPremium"
            inputVariable="Invoke_FEDESIService_InputVariable"
            outputVariable="Invoke_FEDESIService_OutputVariable"
            bpelx:invokeAsDetail="no"/>
    <assign name="Transform_Resp">
      <bpelx:annotation>
        <bpelx:pattern patternName="bpelx:transformation"/>
      </bpelx:annotation>
      <copy>
        <from>ora:doXSLTransformForDoc("xsl/Xform_RespJMS.xsl", $Invoke_FEDESIService_OutputVariable.body, "Consume_Message_InputVariable.body", $Consume_Message_InputVariable.body)</from>
        <to variable="Invoke_PublishESIResptoJMS_InputVariable" part="body"/>
      </copy>
    </assign>
    <invoke name="Invoke_PublishESIResptoJMS"
            partnerLink="PublishESIMECResponseJMS"
            portType="ns3:Produce_Message_ptt" operation="Produce_Message"
            inputVariable="Invoke_PublishESIResptoJMS_InputVariable"
            bpelx:invokeAsDetail="no"/>
  </sequence>
</process>